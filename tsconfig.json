{
  "compilerOptions": {
    "declaration": true,
    "target": "es2018",
    "lib": ["DOM", "ES6", "ES7", "ES2017", "ES2018"],
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "files": [
    "src/utils.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
